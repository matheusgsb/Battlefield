package item;

public class ItemException extends Exception {
	
	private static final long serialVersionUID = -4300643532329392154L;

	/**
	 * Constructor of ItemException
	 * 
	 * @param alias
	 */
	public ItemException (String alias){
		super(alias);
	}
	
	/**
	 * Prints the exception message
	 */
	public void printLog(){
		System.out.println(this.getMessage());
	}
	
}
